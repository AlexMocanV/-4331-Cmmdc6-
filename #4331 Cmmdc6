#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,i,maxi=-1,mini=100000001,a;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        cin>>a;
        if(a>maxi)
        {
            maxi=a;
        }
        if(a<mini)
        {
            mini=a;
        }
    }
    cout<<__gcd(mini,maxi);
    return 0;
}
